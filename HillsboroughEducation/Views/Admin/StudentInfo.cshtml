@model HillsboroughEducation.Models.StudentModel

@{
    ViewBag.Title = "StudentInfo";
}

<script>
    $m = jQuery.noConflict();
</script>
<script type="text/javascript">
    $m(document).ready(function () {
        $m('#tabs').tabs();
    });
</script>

<body>

    <div class="display-field">
        @Html.DisplayFor(model => model.FirstName)
        @Html.DisplayFor(model => model.MiddleName)
        @Html.DisplayFor(model => model.LastName)
        (@Html.DisplayFor(model => model.StudentNumber))
    </div>
    <p></p>
    <div id="tabs">

        <ul>
            <li><a href="#personal">Personal Information</a></li>
            <li>@Html.ActionLink("Financial Information", "StudentFinancialInfo", "Admin", new { id = Model.UserId }, "")</li>
            <li><a href="#question">Questionnaire</a></li>
            <li><a href="#academic">Academic Information</a></li>
            <li><a href="#essay">Student Essay Questions</a></li>
            <li><a href="#scores">Test Scores</a></li>
            <li>@Html.ActionLink("Scholarship Information", "StudentScholarship", "Admin", new { id = Model.UserId }, "")</li>
            <li><a href="#documents">Documents</a></li>
        </ul>

        <div id="personal">
            <h3>Personal Information</h3>
            <div class="row">
                <div class="col-md-4">
                    <b>UserID: @Html.DisplayFor(model => model.UserId)</b>
                </div>
                <div class="col-md-4">
                    <b>Academic Year:  @Html.DisplayFor(model => model.AcademicYear)</b>
                </div>
            </div>
            <p></p>
            <div class="row">
                <div class="col-md-4">
                    First Name:
                    <input type="text" class="form-control" placeholder="@Html.DisplayFor(model => model.FirstName)">
                </div>
                <div class="col-md-4">
                    Social Security #:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-4">
                    Last Name:
                    <input type="text" class="form-control" placeholder="@Html.DisplayFor(model => model.LastName)">
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    Country:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-2">
                    Gender:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-3">
                    Ethnicity:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-4">
                    Date of Birth:
                    <input type="text" class="form-control" placeholder="@Html.DisplayFor(model => model.BirthDate)">
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    School Student ID#:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-4">
                    STARS Generated Student ID #:
                    <input type="text" class="form-control" placeholder="@Html.DisplayFor(model => model.StudentNumber)">
                </div>
                <div class="col-md-4">
                    Address 1:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    City:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-3">
                    State:
                    <select class="form-control" name="state">
                        <option value="AL">Alabama</option>
                        <option value="AK">Alaska</option>
                        <option value="AZ">Arizona</option>
                        <option value="AR">Arkansas</option>
                        <option value="CA">California</option>
                        <option value="CO">Colorado</option>
                        <option value="CT">Connecticut</option>
                        <option value="DE">Delaware</option>
                        <option value="DC">District of Columbia</option>
                        <option value="FL">Florida</option>
                        <option value="GA">Georgia</option>
                        <option value="HI">Hawaii</option>
                        <option value="ID">Idaho</option>
                        <option value="IL">Illinois</option>
                        <option value="IN">Indiana</option>
                        <option value="IA">Iowa</option>
                        <option value="KS">Kansas</option>
                        <option value="KY">Kentucky</option>
                        <option value="LA">Louisiana</option>
                        <option value="ME">Maine</option>
                        <option value="MD">Maryland</option>
                        <option value="MA">Massachusetts</option>
                        <option value="MI">Michigan</option>
                        <option value="MN">Minnesota</option>
                        <option value="MS">Mississippi</option>
                        <option value="MO">Missouri</option>
                        <option value="MT">Montana</option>
                        <option value="NE">Nebraska</option>
                        <option value="NV">Nevada</option>
                        <option value="NH">New Hampshire</option>
                        <option value="NJ">New Jersey</option>
                        <option value="NM">New Mexico</option>
                        <option value="NY">New York</option>
                        <option value="NC">North Carolina</option>
                        <option value="ND">North Dakota</option>
                        <option value="OH">Ohio</option>
                        <option value="OK">Oklahoma</option>
                        <option value="OR">Oregon</option>
                        <option value="PA">Pennsylvania</option>
                        <option value="RI">Rhode Island</option>
                        <option value="SC">South Carolina</option>
                        <option value="SD">South Dakota</option>
                        <option value="TN">Tennessee</option>
                        <option value="TX">Texas</option>
                        <option value="UT">Utah</option>
                        <option value="VT">Vermont</option>
                        <option value="VA">Virginia</option>
                        <option value="WA">Washington</option>
                        <option value="WV">West Virginia</option>
                        <option value="WI">Wisconsin</option>
                        <option value="WY">Wyoming</option>
                    </select>
                </div>
                <div class="col-md-3">
                    County:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")" />
                </div>
                <div class="col-md-3">
                    Postal Code:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    Home Phone:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-4">
                    Work Phone:
                    <input type="text" class="form-control" placeholder=" @Html.DisplayText("") ">
                </div>
                <div class="col-md-4">
                    Cell Phone:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    Email Address/Username:
                    <input type="text" class="form-control" placeholder="@Html.DisplayFor(model => model.UserName)">
                </div>
                <div class="col-md-6">
                    Notes:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
            </div>
        </div>

        <div id="academic">
            <h3>Academic Information</h3>

            Type of Student:

            <div class="row">
                <div class="col-md-6">
                    School Name:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-4">
                    Exp. HS Graduation Date:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-2">
                    GPA:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    Major are of study or program:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
                <div class="col-md-6">
                    Exp. Degree:
                    <input type="text" class="form-control" placeholder="@Html.DisplayText("")">
                </div>
            </div>
        </div>

        <div id="scores">
            <h3>Test Scores</h3>
            <h3>ACT_SCORES_COME_UNDER_THIS_GROUP Scores</h3>
            <h3>SAT_SCORES_COME_UNDER_THIS_GROUP Scores</h3>
            <h3>FAFSA_SCORE Scores</h3>
        </div>

        <div id="question">
            <h3>Opening Questionnaire</h3>
            <div class="row">
                <div class="col-md-12">
                    Which of the following types of student most closely matches you?
                    @Html.DisplayText("")
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    Do you attend a charted school?
                    @Html.DisplayText("")
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    In what grade are you?
                    @Html.DisplayText("")
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    Do you qualify for free or reduced lunch:
                    @Html.DisplayText("")
                </div>
            </div>
        </div>

        <div id="essay">
            <h3>Student Essay Questions</h3>
        </div>

        <div id="documents">
            <h3>Documents</h3>
        </div>

        <p align="right">
            <b>
                @Html.ActionLink("Back to List", "Student", "Admin", "", "")
            </b>
        </p>
    </div>
</body>

