@model HillsboroughEducation.Models.StudentModel

@{
    ViewBag.Title = "StudentInfo";
}

<script>
    $m = jQuery.noConflict();
</script>
<script type="text/javascript">
    $m(document).ready(function () {
        $m('#tabs').tabs();
    });
</script>

<body>

    <div class="display-field">
        @Html.DisplayFor(model => model.FirstName)
        @Html.DisplayFor(model => model.MiddleName)
        @Html.DisplayFor(model => model.LastName)
        (@Html.DisplayFor(model => model.StudentNumber))
    </div>
    <div id="tabs">
        <ul>
            <li><a href="#personal">Personal Info</a></li>
            <li>@Html.ActionLink("Financial Info", "StudentFinancialInfo", "Admin", new { id = Model.UserId }, "")</li>
            <li><a href="#academic">Academic Info</a></li>
            <li><a href="#scores">Test Scores</a></li>
            <li><a href="#question">Questionnaire</a></li>
            <li>@Html.ActionLink("Scholarship Info", "StudentScholarship", "Admin", new { id = Model.UserId }, "")</li>
            <li><a href="#essay">Essay Questions</a></li>
            <li><a href="#documents">Documents</a></li>
        </ul>

        <div id="personal">
            <h3>Personal Information</h3>
            <section>
                UserId:
                @Html.DisplayFor(model => model.UserId)
            </section>
            <section>
                Academic Year:
                @Html.DisplayFor(model => model.AcademicYear)
            </section>
            <section>
                First Name:
                @Html.DisplayFor(model => model.FirstName)
            </section>
            <section>
                Social Security #:
                @Html.DisplayText("")
            </section>
            <section>
                Last Name:
                @Html.DisplayFor(model => model.LastName)
            </section>
            <section>
                Country:
                @Html.DisplayText("")
            </section>
            <section>
                Gender:
                @Html.DisplayText("")
            </section>
            <section>
                Ethnicity:
                @Html.DisplayText("")
            </section>
            <section>
                Date of Birth:
                @Html.DisplayFor(model => model.BirthDate)
            </section>
            <section>
                Country:
                @Html.DisplayText("")
            </section>
            <section>
                Address 1:
                @Html.DisplayText("")
            </section>
            <section>
                City:
                @Html.DisplayText("")
            </section>
            <section>
                State:
                @Html.DisplayText("")
            </section>
            <section>
                County:
                @Html.DisplayText("")
            </section>
            <section>
                Postal Code:
                @Html.DisplayText("")
            </section>
            <section>
                School Student ID#:
                @Html.DisplayText("")
            </section>
            <section>
                STARS Generated Student ID #:
                @Html.DisplayFor(model => model.StudentNumber)
            </section>
            <section>
                Home Phone:
                @Html.DisplayText("")
            </section>
            <section>
                Work Phone:
                @Html.DisplayText("")
            </section>
            <section>
                Cell Phone:
                @Html.DisplayText("")
            </section>
            <section>
                Email Address/Username:
                @Html.DisplayFor(model => model.UserName)
            </section>
            <section>
                Notes:
                @Html.DisplayText("")
            </section>
        </div>

        <div id="academic">
            <h3>Academic Information</h3>
            <section>
                Type of Student:
            </section>
            <section>
                School Name:
                @Html.DisplayText("")
            </section>
            <section>
                Exp. HS Graduation Date:
                @Html.DisplayText("")
            </section>
            <section>
                GPA:
                @Html.DisplayText("")
            </section>
            <section>
                Major are of study or program:
                @Html.DisplayText("")
            </section>
            <section>
                Exp. Degree:
                @Html.DisplayText("")
            </section>
        </div>

        <div id="scores">
            <h3>Test Scores</h3>
            <h4>ACT_SCORES_COME_UNDER_THIS_GROUP Scores</h4>
            <h4>SAT_SCORES_COME_UNDER_THIS_GROUP Scores</h4>
            <h4>FAFSA_SCORE Scores</h4>
        </div>

        <div id="question">
            <h3>Opening Questionnaire</h3>
            <section>
                Which of the following types of student most closely matches you?
                @Html.DisplayText("")
            </section>
            <section>
                Do you attend a charted school?
                @Html.DisplayText("")
            </section>
            <section>
                In what grade are you?
                @Html.DisplayText("")
            </section>
            <section>
                Do you qualify for free or reduced lunch:
                @Html.DisplayText("")
            </section>
        </div>

        <div id="essay">
            <h3>Student Essay Questions</h3>
        </div>

        <div id="documents">
            <h3>Documents</h3>
        </div>

    </div>
    <p align="right"><b>@Html.ActionLink("Back to List", "Student", "Admin", "", "")</b></p>
</body>