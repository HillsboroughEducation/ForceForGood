@model HillsboroughEducation.Models.StudentModel

@{
    ViewBag.Title = "MyInfo";
}
<body>
          <div id="personal">
       <h2>Personal Information</h2>
            <section>
                First Name:
                @Html.DisplayFor(model => model.FirstName)
            </section>
            <section>
                Social Security #:
                @Html.DisplayText("model => model.SocialSecurity")
            </section>
            <section>
                Last Name:
                @Html.DisplayFor(model => model.LastName)
            </section>
            <section>
                Country:
                @Html.DisplayText("model => model.Country")
            </section>
            <section>
                Gender:
                @Html.DisplayText("model => model.Gender")
            </section>
            <section>
                Ethnicity:
                @Html.DisplayText("model => model.Ethnicity")
            </section>
            <section>
                Date of Birth:
                @Html.DisplayFor(model => model.BirthDate)
            </section>
            <section>
                Address:
                @Html.DisplayText("model => model.Address")
            </section>
            <section>
                City:
                @Html.DisplayText("model => model.City")
            </section>
            <section>
                State:
                @Html.DisplayText("model => model.State")
            </section>
            <section>
                County:
                @Html.DisplayText("model => model.County")
            </section>
            <section>
                Postal Code:
                @Html.DisplayText("model => model.PostalCode")
            </section>
            <section>
                School Student ID#:
                @Html.DisplayText("model => model.StudentID")
            </section>
            <section>
                Generated Student ID #:
                @Html.DisplayFor(model => model.StudentNumber)
            </section>
            <section>
                Home Phone:
                @Html.DisplayText("model => model.HomepPhone")
            </section>
            <section>
                Work Phone:
                @Html.DisplayText("model => model.WorkPhone")
            </section>
            <section>
                Cell Phone:
                @Html.DisplayText("model => model.CellPhone")
            </section>
            <section>
                Email Address/Username:
                @Html.DisplayFor(model => model.UserName)
            </section>

              <p>
            @Html.ActionLink("Back to List", "MyInfo", "Reviewer", "", "")
            <a href="#tabs">Top</a>
        </p>
    </div>
</body>
